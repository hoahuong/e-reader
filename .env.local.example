# Upstash Redis Configuration
# Copy file này thành .env.local và điền giá trị thực

# Lấy từ Vercel Dashboard → Settings → Environment Variables
# Hoặc từ Upstash Console → Database → REST API
KV_REST_API_URL=https://your-region-your-name-your-id.upstash.io
KV_REST_API_TOKEN=your-token-here

# Google Drive API (optional)
VITE_GOOGLE_CLIENT_ID=your-client-id-here
VITE_GOOGLE_API_KEY=your-api-key-here
